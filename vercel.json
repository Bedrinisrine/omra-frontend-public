{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/hotels_front/browser",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|json))",
      "dest": "/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

